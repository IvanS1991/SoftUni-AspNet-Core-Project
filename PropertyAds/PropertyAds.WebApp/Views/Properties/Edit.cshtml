@model PropertyFormModel

@{
    ViewData["Title"] = "Редактирай обява";
}

<partial name="_PropertyFormPartial" model="@Model" />

@section Scripts {
    <script>
        var propertyForm = new PropertyForm();

        propertyForm.listenForPreview();
        propertyForm.listenForDistrictAndPropertyType();
        propertyForm.listenForPriceChange();
    </script>

    <partial name="_ValidationScriptsPartial" />
    <partial name="_ValidationScriptsExtensionsPartial" />
}